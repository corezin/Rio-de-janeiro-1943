local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Puxar arma | Rio de Janeiro, 1943",
   LoadingTitle = "Rio de Janeiro, 1943",
   LoadingSubtitle = "by idont_cry",
   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Example Hub"
   },
})

local MainTab = Window:CreateTab("Fuzil", nil) -- Title, Image
local MainSection = MainTab:CreateSection("Main")

Rayfield:Notify({
   Title = "Script executado com sucesso",
   Content = "destrua o RP :]",
   Duration = 5,
   Image = 13047715178,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "Okay!",
         Callback = function()
         print("The user tapped Okay!")
      end
   },
},
})

local Button = MainTab:CreateButton({
    Name = "Hotchkiss M1922",
    Callback = function()
      local args = {
         [1] = "Hotchkiss M1922",
         [2] = 0
     }
     
     game:GetService("ReplicatedStorage"):WaitForChild("CompraEvento"):FireServer(unpack(args))
   end,
})
